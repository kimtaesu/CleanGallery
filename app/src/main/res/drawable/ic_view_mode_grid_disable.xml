<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="36dp"
            android:height="36dp"
            android:viewportHeight="24"
            android:viewportWidth="24">
            <path
                android:name="path"
                android:fillColor="#FF000000"
                android:pathData="M 4 11 L 9 11 L 9 5 L 4 5 L 4 11 Z M 4 18 L 9 18 L 9 12 L 4 12 L 4 18 Z M 10 18 L 15 18 L 15 12 L 10 12 L 10 18 Z M 16 18 L 21 18 L 21 12 L 16 12 L 16 18 Z M 10 11 L 15 11 L 15 5 L 10 5 L 10 11 Z M 16 5 L 16 11 L 21 11 L 21 5 L 16 5 Z" />
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="1000"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="fillColor"
                android:valueFrom="@color/colorAccent"
                android:valueTo="@color/colorDisable"
                android:valueType="colorType" />
        </aapt:attr>
    </target>
</animated-vector>
